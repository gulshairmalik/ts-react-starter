{
	"compilerOptions": {
	  "target": "es5",
	  "module": "esnext",
	  "moduleResolution": "node",
	  "allowJs": true,
	  "checkJs": false,
	  "jsx": "react",
	  "sourceMap": true,
	  "outDir": "./build",
	  "rootDir": "./",
	  "noEmit": false,
	  "strict": true,
	  "noUnusedLocals": true,
	  "noUnusedParameters": true,
	  "noImplicitReturns": true,
	  "noFallthroughCasesInSwitch": true,
	  "experimentalDecorators": true
	},
	"include": [
		 "./src/**/*"
	]
 }